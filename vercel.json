{
  "version": 2,
  "functions": {
    "api/generateCoverSheet.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/update-airtable-attachment.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/supabase-pdf-upload.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/submit-transaction.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/generatePdf.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/sendEmail.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/generateCoverSheet", "destination": "/api/generateCoverSheet.js" },
    { "source": "/api/submit-transaction", "destination": "/api/submit-transaction.js" },
    { "source": "/api/generatePdf", "destination": "/api/generatePdf.js" },
    { "source": "/api/sendEmail", "destination": "/api/sendEmail.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}